{
  "name": "material-converter",
  "version": "0.1.0",
  "description": "An audio converter for video files",
  "main": "public/electron.js",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/murgatt/material-converter.git"
  },
  "author": "ThÃ©o Murgat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/murgatt/material-converter/issues"
  },
  "scripts": {
    "build": "react-scripts build",
    "dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && yarn electron-dev\"",
    "eject": "react-scripts eject",
    "electron-dev": "nodemon --watch public --exec \"electron .\"",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/electron.js",
    "preelectron-pack": "yarn build",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "homepage": "./",
  "dependencies": {
    "@material-ui/core": "^4.8.0",
    "@material-ui/icons": "^4.5.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "electron-is-dev": "^1.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "typeface-roboto": "^0.0.75"
  },
  "devDependencies": {
    "concurrently": "^5.0.1",
    "electron": "^7.1.5",
    "electron-builder": "^21.2.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "wait-on": "^3.3.0"
  },
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
